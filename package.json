{
	"name": "obsidian-kobo-highlights-importer-plugin",
	"version": "4.1.0",
	"type": "module",
	"description": "Import highlights from your Kobo device",
	"main": "src/main.js",
	"scripts": {
		"lint": "eslint . --ext .ts",
		"dev": "watchexec -r -e ts -- node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' mocha 'src/**/*.test.ts'",
		"coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.33.0",
		"@types/chai": "5.2.3",
		"@types/mocha": "10.0.10",
		"@types/node": "22.19.1",
		"@types/sql.js": "1.4.9",
		"@types/uuid": "^11.0.0",
		"@typescript-eslint/eslint-plugin": "^8.31.0",
		"@typescript-eslint/parser": "^8.31.0",
		"builtin-modules": "5.0.0",
		"chai": "6.2.1",
		"esbuild": "0.27.0",
		"eslint": "9.39.1",
		"globals": "^16.3.0",
		"mocha": "11.7.5",
		"nyc": "17.1.0",
		"obsidian": "1.8.7",
		"release-please": "^17.0.0",
		"ts-node": "10.9.2",
		"tslib": "2.8.1",
		"typescript": "5.9.3"
	},
	"dependencies": {
		"@popperjs/core": "2.11.8",
		"@types/better-sqlite3": "7.6.13",
		"esbuild-plugin-wat": "0.2.7",
		"eta": "^4.0.0",
		"moment": "2.30.1",
		"sanitize-filename-ts": "1.0.2",
		"sql.js": "1.8.0",
		"uuid": "^13.0.0"
	},
	"engines": {
		"node": ">=20.0.0 <23.0.0"
	}
}
